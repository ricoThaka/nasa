      <!-- Mode/Group/Dataset/Product/Location selection dropdowns. -->
      <div style="margin-left:auto; margin-right:auto; max-width:95%;"
           class="row w-100 p-1 dark-bg1-sport rounded text-white" >
        <div class="col-lg-6 text-center p-0 rounded" style="background-color: #092743;" >
          <div class="row p-0 m-0 text-center">
            <div class="col-xl-6 p-3 m-0 text-center">
              <h5>Mode:</h5>
              <div class="dropdown light-dropdown-sport show">
                <button
                  class="btn dropdown-toggle text-wrap"
                  style="width:90%;"
                  type="button"
                  id="modeDropdownMenuButton"
                  data-bs-toggle="dropdown"
                  aria-haspopup="true"
                  aria-expanded="false"
                  >
                    Select
                </button>
                <div
                  id="datamode_dropdown"
                  class="dropdown-menu"
                  aria-labelledby="modeDropdownMenuButton"
                  ></div>
              </div>
            </div>
            <div class="col-xl-6 p-3 m-0 text-center">
              <h5>Dataset:</h5>
              <div class="dropdown light-dropdown-sport show">
                <button
                  class="btn dropdown-toggle text-wrap"
                  style="width:90%;"
                  type="button"
                  data-bs-toggle="collapse"
                  data-bs-target="#datasetCollapsible"
                  aria-expanded="false"
                  aria-controls="datasetCollapsible"
                  >
                  <!-- <button class="btn dropdown-toggle" type="button" id="datasetDropdownMenuButton" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false" > -->
                  <span id="datasetDropdownButtonText">Select</span>
                </button>
                <div
                  class="collapse w-100"
                  id="datasetCollapsible"
                  style="position:absolute; z-index:1000;"
                  >
                  <div
                    class="accordion rounded mx-3 mx-sm-4 mx-md-3"
                    id="dataset_accordion"
                    style="height: 400px; overflow-y:scroll; "
                    ></div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="col-lg-6 text-center p-0 rounded" style="background-color: #092743;">
          <div class="row p-0 m-0 text-center">
            <div class="col-xl-6 p-3 m-0 text-center">
              <h5>Product:</h5>
              <div class="dropdown light-dropdown-sport show">
                <button
                  class="btn dropdown-toggle text-wrap"
                  style="width:90%;"
                  type="button"
                  id="productDropdownMenuButton"
                  data-bs-toggle="dropdown"
                  aria-haspopup="true"
                  aria-expanded="false"
