      <!-- Window frame with main image and buttons -->
      <div class="row mt-3 align-items-center justify-content-between dark-bg1-sport rounded" >
        <div
          id="fsContainer"
          class="p-1 col-sm-12 dark-bg1-sport rounded"
        >
          <div id="fsInnerContainer">
          <!-- Active image frame -->
          <div id="main_image_container" class="p-2 m-0 rounded" style="background-color: #092743" >

            <img id="main_image" class="jusify-content-center img-fluid d-block mx-auto zoom not-fullscreen rounded" src="#">
              <div class="mx-auto" id="img_progress_container">
                <div id="img_progress" class="progress w-75" style="margin-left: auto; margin-right: auto;" >
                  <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%" >
                    0%
                  </div>
                </div>
              </div>
            </img>

          </div>

          <!-- Image status and manipulation section -->
          <div class="p-3 mt-2 rounded text-white" style="background-color: #092743">
            <!--  Current image date label -->
            <div class="row">
              <h6 id="time_label" class="sport-head text-center"></h6>
            </div>
            <!-- Static image selection tickers -->
            <div class="row mt-0 flex-grow-1 d-flex justify-content-center">
              <div id="ticker_container" class="d-flex flex-wrap p-2 justify-content-center align-items-center" ></div>
            </div>

            <!-- Pause/Play/Seek buttons-->
            <div class="row mt-1 mx-auto justify-content-center">
                <button data-placement="right" class="btn m-1 light-button-sport" onclick="decrement()" style="display: table-cell; vertical-align: middle; width:38px; height:38px;">
                  <span class="fa-solid fa-chevron-left fa-xs"></span>
                </button>
                <button id="playpause" onclick="toggleImageCycle()" type="button" class="btn m-1 light-button-sport" style="width:76px; height:38px;">
                  <span class="fa-solid fa-play fa-xs"></span>
                  <span class="fa-solid fa-pause fa-xs"></span>
                </button>
                <button data-placement="left" class="btn m-1 light-button-sport" onclick="increment()" style="display: table-cell; vertical-align: middle; width:38px; height:38px;">
                  <span class="fa-solid fa-chevron-right fa-xs"></span>
                </button>
            </div>

            <!-- Auto-update switch -->
            <div class="row py-2 mt-1">
              <div class="col d-flex justify-content-center">
                <div class="form-check form-switch">
                  <input type="checkbox" class="form-check-input" id="enableAutoUpdate">
                  <label class="form-check-label" for="enableAutoUpdate">Auto Update</label>
                </div>
              </div>
            </div>

            <!-- Frame delay, starting/ending image, and image count. -->
            <div class="row mt-1 mx-auto justify-content-center text-center">
              <div class="col-xl-3 my-auto d-flex flex-column justify-content-center light-dropdown-sport show" style="max-width:250px">
                  <label class="mt-2 mb-0 d-block" for="frame_delay">Frame delay (ms):</label>
                  <input step="5" type="number" class="m-1 w-75 form-control mx-auto text-center" style="font-size:80%;" value="100" min="10" max="5000" id="frame_delay" />
              </div>
              <div class="col-xl-3 my-auto d-flex flex-column justify-content-center light-dropdown-sport show" style="max-width:250px">
                  <label class="mt-2 mb-0 d-block">Starting Image:</label>
                  <button class="btn m-1 dropdown-toggle" type="button"
                          id="startDropdownMenuButton" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false" style="font-size: 85%" >
                    Start Time
                  </button>
                  <div id="timestart_dropdown" class="dropdown-menu img-dropdown-select-font-sz"
                       aria-labelledby="startDropdownMenuButton" style="font-family:monospace;" ></div>
              </div>
              <div class="col-xl-3 my-auto d-flex flex-column justify-content-center light-dropdown-sport show" style="max-width:250px">
                  <label class="mt-2 mb-0 d-block">Ending Image:</label>
                  <button class="btn m-1 dropdown-toggle" type="button"
                          id="endDropdownMenuButton" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false" style="font-size: 85%" >
                    End Time
                  </button>
                  <div id="timeend_dropdown" class="dropdown-menu img-dropdown-select-font-sz"
                       aria-labelledby="endDropdownMenuButton" style="font-family: monospace;" ></div>
              </div>
              <div class="col-xl-3 my-auto d-flex flex-column justify-content-center light-dropdown-sport show" style="max-width:250px">
                  <label class="mt-2 mb-0 d-block" for="images_to_display" >Image Count:</label >
                  <input class="m-1 w-75 form-control mx-auto text-center" type="number" value="20" id="images_to_display" style="font-size:80%; align-items: center; background-color: #c7c7c7; color: #949494; " disabled />
              </div>
            </div>

            <!-- GIF Download and information modal -->
            <div class="row mt-1 mx-auto">
              <div class="col-md-4 my-auto d-flex flex-column justify-content-start light-dropdown-sport show">
                <div class="help-info-container text-center d-md-flex justify-content-start mt-4 mt-md-0" >
                  <span class="button-tooltip" data-toggle="tooltip" data-placement="top">
                  <button type="button" data-placement="left" class="btn m-1 light-button-sport" data-bs-toggle="modal" data-bs-target="#gif_modal" style="font-size:80%;">
                    Download GIF
                  </button>
                  </span>
                  </div>
              </div>
              <div class="col-md-4 my-auto d-flex flex-column text-center light-dropdown-sport show">
              </div>
              <div class="col-md-4 my-auto d-flex flex-column justify-content-center light-dropdown-sport show">
                <div class="help-info-container text-center d-md-flex justify-content-end mt-4 mt-md-0" >
                  <span class="button-tooltip" data-toggle="tooltip" data-placement="top" title="Click for help." >
                    <button type="button" class="btn btn-sm light-button-sport" data-bs-toggle="modal" data-bs-target="#help-modal" >
                      <i class="fa-solid fa-circle-question"></i>
                    </button>
                  </span>
                </div>
              </div>
            </div>

            <!-- GIF creation and static image looping options -->
            <div id="start-stop" class="row mt-1 mx-auto text-center">
              <div
                class="col-md-3 my-auto d-flex flex-column justify-content-center light-dropdown-sport show"
              >
                <div class="row h-50 justify-content-center">
                  <div id="auto_off" class="row text-center dropdown" style="display: block" >

                  </div>
                </div>
              </div>
            </div>
          </div>
          </div>
        </div>
        <!-- Include the info and gif creation modals -->
        <div class="modal" id="gif_modal">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header dark-bg1-sport">
        <h4 class="modal-title text-white">Generate Animation</h4>
        <button
          type="button"
          class="btn-close"
          style="background-color:white;"
          data-bs-dismiss="modal"
        ></button>
      </div>
      <div class="modal-body text-dark">
        <p class="text-center">
          Generate an animation from the selected data. Frames with red buttons
          are excluded.
        </p>
        <div class="w-100 text-center">
          <div class="col">
            <div class="row">
              <p class="mb-0 mx-auto">Slide to choose delay between frames:</p>
            </div>
            <div class="row mb-3 mt-2">
              <input
                type="range"
                min="10"
                max="5000"
                value="100"
                class="mx-auto"
                id="delaySlider"
                style="width: 50%"
              />
            </div>
            <div class="row">
              <p class="mb-0 mx-auto">Or enter delay below:</p>
            </div>
            <div class="mb-3">
              <div class="mx-auto">
                <div class="d-inline-block">
                  <input
                    type="number"
                    class="mx-auto"
                    id="gifDelayInput"
                    min="10"
                    max="5000"
                  />
                </div>
                <div class="d-inline-block">
                  <p>ms</p>
                </div>
                <div class="validation-error-container">
                  <p
                    id="gif-delay-validation-error"
                    style="color: red; visibility: hidden"
                  >
                    Out of bounds! Enter a value between 10 and 5000.
                  </p>
                </div>
              </div>
            </div>
          </div>
          <p>
            <input type="checkbox" id="stutter_gif" value="stutter_checkbox" />
            <label for="stutter_gif">Lag Final Frame</label>
          </p>
        </div>
        <button
          id="generate_gif_button"
          onclick="makeGif()"
          class="d-block btn light-button-sport mx-auto mb-2"
        >
          Get Animation
        </button>
        <p
          id="animation_loading"
          style="display: none"
          class="container text-center mt-3"
        >
          Loading...
        </p>
        <img
          id="animation_image"
          download="SPoRTAnimation"
          class="img-fluid d-block mx-auto"
          style="display: none"
          src=""
        />
        <button
          id="download_gif_button"
          onclick="downloadGif()"
          class="btn light-button-sport mx-auto mb-2 d-none mt-3"
        >
          Download Animation
        </button>
      </div>
      <div class="modal-footer">
        <button
          type="button"
          class="btn light-button-sport"
          data-bs-dismiss="modal"
        >
          Close
        </button>
      </div>
    </div>
  </div>
</div>

<!-- Modal -->
<div
  class="modal fade"
  id="help-modal"
  tabindex="-1"
  aria-labelledby="helpModalLabel"
  aria-hidden="true"
>
  <div class="modal-dialog modal-xl">
    <div class="modal-content">
      <div class="modal-header dark-bg1-sport">
        <h5 class="modal-title text-white" id="helpModalLabel">
          How to use the SPoRT Viewer Site
        </h5>
        <button
          type="button"
          style="background-color:white;"
          class="btn-close"
          data-bs-dismiss="modal"
          aria-label="Close"
        >
          <span class="text-white" aria-hidden="true"></span>
        </button>
      </div>
      <div class="modal-body">
        <span class="help-content">
          <ul class="nav nav-pills" id="myTab" role="tablist">
            <li class="nav-item" role="presentation">
              <a
                class="nav-link active"
                id="intro-tab"
                data-bs-toggle="tab"
                href="#intro"
                role="tab"
                aria-controls="intro"
                aria-selected="true"
                >Intro</a
              >
            </li>
            <li class="nav-item" role="presentation">
              <a
                class="nav-link"
                id="data-product-selection-tab"
                data-bs-toggle="tab"
                href="#data-product-selection"
                role="tab"
                aria-controls="data-product-selection"
                aria-selected="false"
                >Data Product Selection</a
              >
            </li>
            <li class="nav-item" role="presentation">
              <a
                class="nav-link"
                id="image-loop-control-tab"
                data-bs-toggle="tab"
                href="#image-loop-control"
                role="tab"
                aria-controls="image-loop-control"
                aria-selected="false"
                >Image Loop Control</a
              >
            </li>
            <li class="nav-item" role="presentation">
              <a
                class="nav-link"
                id="gif-generation-tab"
                data-bs-toggle="tab"
                href="#gif-generation"
                role="tab"
                aria-controls="gif-generation"
                aria-selected="false"
                >GIF Generation</a
              >
            </li>
          </ul>
          <div class="tab-content" id="myTabContent">
            <div
              class="tab-pane fade show active"
              id="intro"
              role="tabpanel"
              aria-labelledby="intro-tab"
            >
              <div class="help-pane-container">
                <div class="help-pane-title">
                  <h5 class="text-dark pt-4 text-center">
                    <u>SPoRT Viewer Introduction</u>
                  </h5>
                </div>
                <div class="help-pane-body mt-4">
                  <p class="text-dark">
                    The SPoRT Viewer is an interactive tool that displays
                    dynamic imagery from various Earth Science resources.
                    Imagery is categorized by dataset, product and location (if
                    applicable). The SPoRT Viewer contains several tools which
                    will be described in this guide.
                  </p>
                  <p class="text-dark">
                    Imagery can be manually toggled through frame by frame, or
                    can be animated. Animated data can be downloaded as a GIF
                    file.
                  </p>
                  <p class="text-dark">
                    Navigate through the tabs above to learn about the Viewer's
                    tools and features.
                  </p>
                </div>
              </div>
            </div>
            <div
              class="tab-pane fade"
              id="data-product-selection"
              role="tabpanel"
              aria-labelledby="data-product-selection-tab"
            >
              <div class="help-pane-container">
                <div class="help-pane-title">
                  <h5 class="text-dark pt-4 text-center">
                    <u>Data Product Selection</u>
                  </h5>
                </div>
                <div class="help-pane-body mt-4">
                  <img
                    src="media-dev/dataProductSelection.png"
                    class="rounded"
                    style="width: 100%"
                    alt="Three dropdown buttons to select different datasets, products and locations (where applicable)."
                  />
                  <p class="text-dark mt-3">
                    The data selection panel pictured above enables filtering by
                    dataset, product, and location. The location dropdown will
                    be conditionally populated for certain dataset-product
                    combinations.
                  </p>
                </div>
              </div>
            </div>
            <div
              class="tab-pane fade"
              id="image-loop-control"
              role="tabpanel"
              aria-labelledby="image-loop-control-tab"
            >
              <div class="help-pane-container">
                <div class="help-pane-title">
                  <h5 class="text-dark pt-4 text-center">
                    <u>Image Control Loop</u>
                  </h5>
                </div>
                <div class="help-pane-body mt-4">
                  <img
                    src="media-dev/imageControlLoopPanel.png"
                    class="rounded"
                    style="width: 100%"
                    alt="Three dropdown buttons to select different datasets, products and locations (where applicable)."
                  />
                  <p class="text-dark mt-3">
                    The Image Control Loop Panel contains several elements:
                  </p>
                  <ul class="text-dark border-bottom border-dark py-2">
                    <li>Datetime Label</li>
                    <li>Start Time Button</li>
                    <li>End Time Button</li>
                    <li>Image Sequence Bar</li>
                    <li>Toggle Forward</li>
                    <li>Toggle Backward</li>
                    <li>Play/Pause Button</li>
                    <li>Animation Delay Input</li>
                    <li>New Data Check Period Input</li>
                    <li>Automatic Update Switch</li>
                    <li>Download GIF Button</li>
                  </ul>
                  <div class="img-ctrl-loop-item">
                    <h6 class="text-dark">Datetime Label</h6>
                    <p class="text-dark">
                      Describes the date and UTC time the image was generated .
                    </p>
                  </div>
                  <div class="img-ctrl-loop-item">
                    <h6 class="text-dark">Start Time Button</h6>
                    <p class="text-dark">
                      Provides a selection of times at which the imagery loop
                      will begin. The default start time is selected to ...
                    </p>
                    <p class="text-danger">
                      Please note the the selected start time must be earlier
                      than the selected end time.
                    </p>
                  </div>
                  <div class="img-ctrl-loop-item">
                    <h6 class="text-dark">End Time Button</h6>
                    <p class="text-dark">
                      Provides a selection of times at which the imagery loop
                      will end. The default end time is selected to ...
                    </p>
                    <p class="text-danger">
                      Please note the the selected end time must be later than
                      the selected start time.
                    </p>
                  </div>
                  <div class="img-ctrl-loop-item">
                    <h6 class="text-dark">Image Sequence Bar</h6>
                    <img
                      src="media-dev/imageControlLoopExcludeImage.png"
                      class="rounded mb-2"
                      style="width: 100%"
                      alt="Another section containing the following. A date and time stamp of the satellite image selected. A horizontal scrollbar with white boxes indicated the images in the sequence. A blue box indicated the current image that is being displayed in the sequence. And a red box indicating an image that is to be skipped when looping through the sequence of images."
                    />
                    <p class="text-dark">
                      The image sequence bar, pictured above, displays the
                      number of images that will be displayed based on your
                      input selections for start time and end time, or for
                      checking the "Automatically Update" checkbox.
                    </p>
                    <p class="text-dark">
                      The blue square indicates the currently displayed image.
                      To display a specific image in the sequence, double click
                      the image's square. A single click will cause a white box
                      to turn red, indicating the selected frame will be skipped
                      within the sequence and left out of generated GIF
                      animations.
                    </p>
                  </div>
                  <div class="img-ctrl-loop-item">
                    <h6 class="text-dark">Toggle Forward</h6>
                    <p class="text-dark">
                      The Toggle Forward Button represented by the right
                      pointing chevron steps ahead in the image sequence.
                    </p>
                  </div>
                  <div class="img-ctrl-loop-item">
                    <h6 class="text-dark">Toggle Backward</h6>
                    <p class="text-dark">
                      The Toggle Backward Button represented by the left
                      pointing chevron steps backward in the image sequence.
                    </p>
                  </div>
                  <div class="img-ctrl-loop-item">
                    <h6 class="text-dark">Play/Pause Button</h6>
                    <p class="text-dark">
                      Clicking this button will play or pause a continuous
                      playback loop of satellite images.
                    </p>
                  </div>
                  <div class="img-ctrl-loop-item">
                    <h6 class="text-dark">Animation Delay Input</h6>
                    <p class="text-dark">
                      The Animation delay input box controls the rate of change
                      between images in the image sequence.
                    </p>
                  </div>
                  <div class="img-ctrl-loop-item">
                    <h6 class="text-dark">New Data Check Period Input</h6>
                    <p class="text-dark">
                      This input field controls the time (in minutes), to check
                      the server for new images to be displayed. This will
                      update the timestamps of images available in the "Start
                      Time" and "End Time" dropdowns. It will also affect the
                      images that are displayed when the "Automatically Update"
                      checkbox is checked.
                    </p>
                  </div>
                  <div class="img-ctrl-loop-item">
                    <h6 class="text-dark">Automatic Update Switch</h6>
                    <p class="text-dark">
                      The automatic update switch, when checked, will cause the
                      "Start Time" and "End Time" dropdowns to disappear and an
                      "Images to Display" input field to appear. This feature
                      will display the latest "N" images, where "N" is the
                      number entered in the "Images to Display" field. This
                      feature uses the "New data check period" input field to
                      determine how often to check the server for new images.
                      Every time there is a new image, the image sequence is
                      updated and the newest image is appended and the oldest
                      image is discarded. The change of input field values when
                      checking on this checkbox is depicted below.
                    </p>
                    <img
                      src="media-dev/ImageControlLoopAutoUpdate.png"
                      class="rounded mb-2"
                      style="width: 100%"
                      alt="Clicking on the automatic update checkbox causes the start time and end time dropdowns to disappear and the images to display input field to appear."
                    />
                  </div>
                  <div class="img-ctrl-loop-item mt-3">
                    <h6 class="text-dark">Download GIF Button</h6>
                    <p class="text-dark">
                      The Download GIF Button opens the GIF creator tool. For
                      more information on this feature, click the "GIF
                      Generation" tab at the top of this pop-up.
                    </p>
                  </div>
                </div>
              </div>
            </div>
            <div
              class="tab-pane fade"
              id="gif-generation"
              role="tabpanel"
              aria-labelledby="gif-generation-tab"
            >
              <div class="help-pane-container">
                <div class="help-pane-title">
                  <h5 class="text-dark pt-4 text-center">
                    <u>GIF Generation</u>
                  </h5>
                </div>
                <div class="help-pane-body mt-4">
                  <img
                    class="d-block mx-auto rounded"
                    src="media-dev/gifImageCreatorPre.png"
                    style="width: 40%"
                    alt="A modal with option to create a gif for a sequence of selected satellite images."
                  />
                  <p class="text-dark mt-3">
                    The GIF creator converts the current Viewer's animation
                    configuration into a downloadable GIF file. To change the
                    lag period between frames, use the slider or input a value
                    directly into the text box. To include an additional period
                    of lag time with the final frame, use the "Lag Final Frame"
                    checkbox. Click the "Generate Animation" button to create
                    the GIF and the "Download Animation" button to save the
                    file.
                  </p>
                  <img
                    class="d-block mx-auto rounded"
                    src="media-dev/gifImageCreatorPost.png"
                    style="width: 40%"
                    alt="GIF generation modal view once the gif has been generated. An image is displayed with a loop of the image sequence and a download button is available."
                  />
                </div>
              </div>
            </div>
          </div>
        </span>
      </div>
